# CLAWrity Environment Variables
# Copy this file to .env and fill in your values

# fal.ai API key for companion image generation (OPTIONAL)
# Get yours at: https://fal.ai/dashboard/keys
# If not set, CLAWrity works in text-only mode (no visual generation)
FAL_KEY=

# OpenClaw Gateway token (OPTIONAL)
# Only needed if openclaw CLI is not installed
OPENCLAW_GATEWAY_TOKEN=

# Custom companion image URL (OPTIONAL)
# Override the default companion reference image
CLAWRITY_COMPANION_IMAGE=
